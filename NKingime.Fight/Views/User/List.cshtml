@model IEnumerable<NKingime.Entity.User>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table>
    <thead>
        <tr>
            <th>用户名</th>
            <th>昵称</th>
            <th>性别</th>
            <th>手机号</th>
            <th>注册日期</th>
            <th>是否启用</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Nickname</td>
                <td>@user.Gender</td>
                <td>@user.Mobile</td>
                <td>@user.RegisterDate</td>
                <td>@user.Enabled</td>
            </tr>
        }
    </tbody>
</table>
