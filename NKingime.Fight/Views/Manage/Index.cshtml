@model NKingime.Entity.User
@using NKingime.Core.Extentsion;
    @{
        Layout = null;
    }
    <!DOCTYPE html>
    <html lang="zh-CN">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>NKingime - 后台管理</title>
        <link href="/static/css/bootstrap/bootstrap.css" rel="stylesheet" />
        <link href="/static/css/font-awesome.min.css" rel="stylesheet" />
        <link href="/static/css/pace/dataurl.css" rel="stylesheet" />
        <link href="/static/css/module/manage.css" rel="stylesheet" type="text/css" />
        <link href="/static/css/jquery-confirm.min.css" rel="stylesheet" type="text/css" />
        <link href="/static/css/loading/loading.css" rel="stylesheet" type="text/css" />
    </head>

    <body>
        <nav class="navbar navbar-inverse navbar-fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle show pull-left" data-target="sidebar">
                        <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                    </button>
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                            aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand">NKingime - 后台管理</a>
                </div>
                <div id="navbar" class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-user fa-fw"></i>@Model.Username<span class="caret"></span></a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="top-right1.html">top-right1.html</a></li>
                                <li class="divider"></li>
                                <li><a href="top-right2.html">top-right2.html</a></li>
                                <li class="divider"></li>
                                <li><a href="javascript:void(0);" onclick="_$manage.logout();"><i class="fa fa-sign-out fa-fw"></i>注销</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div class="container-fluid all">
            <div class="sidebar">
                <ul class="nav">
                    @foreach (var module in (List<Module>)ViewBag.ModuleList)
                    {
                        if (module.Childs.Count > 0)
                        {
                            <li class="has-sub">
                                <a href="@module.Url.IfNullOrWhiteSpace("javascript:void(0);")"><span>@module.Name</span><i class="fa fa-caret-right fa-fw pull-right"></i></a>
                                <ul class="sub-menu">
                                    @foreach (var child in module.Childs)
                                    {
                                        <li><a href="@child.Url.IfNullOrWhiteSpace("javascript:void(0);")"><i class="fa fa-fw"></i>@child.Name</a></li>
                                    }
                                </ul>
                            </li>
                        }
                        else
                        {
                            <li><a href="@module.Url.IfNullOrWhiteSpace("javascript:void(0);")">@module.Name</a></li>
                        }
                    }
                </ul>
            </div>
            <div class="maincontent row form-horizontal">
                <!--我是主要内容 start-->
                <ul class="breadcrumb">
                    <li class="active">UI库</li>
                </ul>
                <div class="col-sm-12">

                </div>
                <!--我是主要内容 end-->
            </div>
        </div>
        <script src="/static/js/jquery-1.10.2.min.js"></script>
        <script src="/static/js/bootstrap/bootstrap.min.js"></script>
        <script src="/static/js/pace/pace.min.js"></script>
        <script src="/static/js/jquery.scrolltopcontrol/scrolltopcontrol.js" type="text/javascript"></script>
        <script src="/static/js/jquery.hammer/hammer.min.js" type="text/javascript"></script>
        <script src="/static/js/jquery.hammer/jquery.hammer.js" type="text/javascript"></script>
        <script src="/static/js/jquery.extend.js" type="text/javascript"></script>
        <script src="/static/js/jquery-confirm.min.js" type="text/javascript"></script>
        <script src="/static/js/loading/loading.js" type="text/javascript"></script>
        <script src="/static/js/jquery.ajax.js" type="text/javascript"></script>
        <script src="/static/js/module/manage.js" type="text/javascript"></script>
    </body>
</html>